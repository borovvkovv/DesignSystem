{
  "name": "design-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:production": "run-p restore-packages type-check build-only:production",
    "build:development": "run-p restore-packages type-check build-only:development",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit-coverege": "vitest run --coverage",
    "restore-packages": "yarn install",
    "build-only:production": "vite build",
    "build-only:development": "vite build --mode development",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@tanstack/vue-query": "^5.36.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.6.8",
    "chart.js": "^4.4.2",
    "date-fns": "^2.30.0",
    "imask": "^7.6.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "tailwind-merge": "^2.3.0",
    "vue": "^3.4.27",
    "vue-chartjs": "^5.3.1",
    "vue-imask": "^7.6.0",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@babel/types": "^7.24.5",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.12.12",
    "@types/string-hash": "^1.1.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/compiler-dom": "^3.4.27",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/server-renderer": "^3.4.27",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.0.0",
    "lodash.debounce": "^4.0.8",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.77.2",
    "start-server-and-test": "^2.0.3",
    "string-hash": "^1.1.3",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vitest-canvas-mock": "^0.3.3",
    "vue-tsc": "^2.0.19"
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
